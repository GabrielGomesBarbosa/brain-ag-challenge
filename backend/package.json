{
  "name": "test-challenge",
  "version": "0.5.3",
  "main": "dist/index.js",
  "author": "Gabriel Gomes Barbosa",
  "license": "ISC",
  "scripts": {
    "start:dev": "node --env-file=.env.local dist/index.js",
    "dev": "tsx watch --env-file=.env.local src/index.ts",
    "prisma:generate": "prisma generate",
    "migrate:dev": "dotenvx run --env-file=.env.local -- prisma migrate dev",
    "seed:dev": "dotenvx run --env-file=.env.local -- prisma db seed",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./prisma/seeds/index"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "express": "^4.21.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.38.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.13",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.2"
  }
}
