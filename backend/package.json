{
  "name": "brain-ag-challenge",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "author": "Gabriel Gomes Barbosa",
  "license": "ISC",
  "scripts": {
    "start:dev": "node --env-file=.env.local dist/index.js",
    "dev": "tsx watch --env-file=.env.local src/index.ts",
    "migrate:dev": "dotenvx run --env-file=.env.local -- prisma migrate dev",
    "seed:dev": "dotenvx run --env-file=.env.local -- prisma db seed",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./prisma/seeds/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.38.0",
    "@types/express": "^5.0.0",
    "@types/node": "^20.16.13",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.2"
  }
}
